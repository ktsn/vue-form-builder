{
  "name": "vue-form-builder",
  "version": "0.0.0",
  "author": "katashin",
  "description": "Form builder on Vue.js",
  "keywords": [
    "form",
    "builder",
    "Vue.js"
  ],
  "license": "MIT",
  "main": "dist/vue-form-builder.cjs.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/ktsn/vue-form-builder",
  "bugs": "https://github.com/ktsn/vue-form-builder/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/ktsn/vue-form-builder.git"
  },
  "scripts": {
    "prepublishOnly": "npm run release",
    "clean": "rm -rf dist .tmp",
    "build": "run-s build:ts build:cjs build:dev build:prod",
    "build:ts": "tsc -p ./",
    "build:cjs": "rollup -c scripts/rollup.config.js --environment BUILD:cjs",
    "build:dev": "rollup -c scripts/rollup.config.js --environment BUILD:dev",
    "build:prod": "rollup -c scripts/rollup.config.js --environment BUILD:prod | uglifyjs -mc warnings=false --comments -o dist/vue-form-builder.min.js",
    "build:test": "cross-env NODE_ENV=test webpack --config scripts/webpack.config.unit.js",
    "watch:test": "cross-env NODE_ENV=test webpack -w --config scripts/webpack.config.unit.js",
    "example": "npm run build:ts && webpack-dev-server --config example/webpack.config.js --hot --open",
    "testem": "testem",
    "testem:ci": "testem ci --launch PhantomJS",
    "test": "npm run test:unit",
    "test:unit": "run-s build:test testem:ci",
    "test:dev": "run-p watch:test testem",
    "release": "run-s test clean build"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/power-assert": "^1.4.29",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "glob": "^7.1.2",
    "npm-run-all": "^4.0.2",
    "power-assert": "^1.4.4",
    "rollup": "^0.42.0",
    "rollup-plugin-replace": "^1.1.1",
    "sass-loader": "^6.0.6",
    "testdouble": "^3.2.5",
    "testem": "^1.16.2",
    "ts-loader": "^2.3.4",
    "typescript": "^2.5.3",
    "uglify-js": "^3.0.15",
    "vue": "^2.5.2",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.5.2",
    "vue-test-utils": "^1.0.0-beta.1",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.9.1",
    "webpack-espower-loader": "^1.0.2"
  }
}
